{
	"version": 3,
	"routes": [
		{
			"src": "/features",
			"dest": "/features/"
		},
		{
			"src": "/features/",
			"status": 308,
			"headers": {
				"Location": "/features"
			}
		},
		{
			"src": "/features/paddock_path",
			"dest": "/features/paddock_path/"
		},
		{
			"src": "/features/paddock_path/",
			"status": 308,
			"headers": {
				"Location": "/features/paddock_path"
			}
		},
		{
			"src": "/features/pipeline",
			"dest": "/features/pipeline/"
		},
		{
			"src": "/features/pipeline/",
			"status": 308,
			"headers": {
				"Location": "/features/pipeline"
			}
		},
		{
			"src": "/pricing",
			"dest": "/pricing/"
		},
		{
			"src": "/pricing/",
			"status": 308,
			"headers": {
				"Location": "/pricing"
			}
		},
		{
			"src": "/_app/immutable/.+",
			"headers": {
				"cache-control": "public, immutable, max-age=31536000"
			}
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "/.*",
			"dest": "/fn"
		}
	],
	"overrides": {
		"index.html": {
			"path": ""
		},
		"features.html": {
			"path": "features"
		},
		"features/paddock_path.html": {
			"path": "features/paddock_path"
		},
		"features/pipeline.html": {
			"path": "features/pipeline"
		},
		"pricing.html": {
			"path": "pricing"
		}
	}
}